# Firebase Configuration
# Get these from your Firebase Console > Project Settings
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_auth_domain_here
VITE_FIREBASE_PROJECT_ID=your_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_storage_bucket_here
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here

# Backend API
VITE_API_BASE_URL=http://localhost:9000

# Google Login (Optional - Firebase handles this automatically)
# Only needed if you want to use Google services outside of Firebase Auth
VITE_GOOGLE_LOGIN_CLIENT_ID=your_google_login_client_id_here

# Google Calendar API
# Get these from Google Cloud Console > APIs & Services > Credentials
# 1. Create an OAuth 2.0 Client ID (Web application)
# 2. Add authorized JavaScript origins: http://localhost:5173, http://localhost:5175
# 3. Add authorized redirect URIs: http://localhost:5173, http://localhost:5175
# 4. Enable Google Calendar API in your Google Cloud Console
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_GOOGLE_CALENDAR_CLIENT_ID=your_google_calendar_client_id_here
